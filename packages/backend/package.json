{
  "name": "@gemini-ui/backend",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "tsx src/db/migrate.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@gemini-ui/shared": "file:../shared",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.5.0",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.0",
    "node-pty": "^1.0.0",
    "better-sqlite3": "^12.2.0",
    "drizzle-orm": "^0.38.5",
    "zod": "^3.23.8",
    "dotenv": "^16.4.7",
    "validator": "^13.12.0",
    "file-type": "^19.8.0",
    "sharp": "^0.34.2",
    "cors": "^2.8.5",
    "multer": "^2.0.1",
    "@paralleldrive/cuid2": "^2.2.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.10",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/ws": "^8.5.13",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.12",
    "@types/validator": "^13.12.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.8",
    "supertest": "^7.0.0",
    "drizzle-kit": "^0.29.1",
    "@types/supertest": "^6.0.2"
  }
}
